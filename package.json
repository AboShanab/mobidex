{
    "name": "mobidex",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "prepare": "npm run patch:isomorphic-fetch && npm run patch:web3",
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "patch:isomorphic-fetch": "./patches/isomorphic-fetch.sh",
        "patch:web3": "./patches/web3.sh",
        "build:android:release": "pushd android; read -sp 'If you would like to sign the APK, please add your store passphrase now: ' MOBIDEX_RELEASE_STORE_PASSWORD; ./gradlew clean assembleRelease -PMOBIDEX_RELEASE_STORE_PASSWORD=${MOBIDEX_RELEASE_STORE_PASSWORD} -PMOBIDEX_RELEASE_KEY_PASSWORD=${MOBIDEX_RELEASE_STORE_PASSWORD} -PMOBIDEX_SIGN=1; popd android;",
        "build:ios:release": "echo 'You need to build an archive via XCode!'"
    },
